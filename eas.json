{
  "cli": {
    "version": ">= 0.52.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m1-medium",
        "image": "latest",
        "credentialsSource": "remote",
        "buildConfiguration": "Release"
      },
      "env": {
        "EXPO_BUNDLE_IOS": "true",
        "EXPO_USE_PREBUILD_IOS": "true",
        "EXPO_SKIP_BUNDLING": "1"
      },
      "prebuildCommand": "./scripts/prepare-ios-bundle.sh && echo 'Bundle preparation completed'"
    },
    "app-store": {
      "extends": "production",
      "distribution": "store",
      "ios": {
        "resourceClass": "m1-medium",
        "image": "latest",
        "credentialsSource": "remote",
        "buildConfiguration": "Release"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "jayg44@sbcglobal.net",
        "ascAppId": "6747609548",
        "appleTeamId": "H82MG3CLF3",
        "bundleIdentifier": "com.forbesyprojects.deductly"
      },
      "android": {
        "serviceAccountKeyPath": "path/to/api-key.json",
        "track": "production"
      }
    },
    "preview": {
      "ios": {
        "appleId": "jayg44@sbcglobal.net",
        "ascAppId": "6747609548",
        "appleTeamId": "H82MG3CLF3",
        "bundleIdentifier": "com.forbesyprojects.deductly"
      },
      "android": {
        "serviceAccountKeyPath": "path/to/api-key.json",
        "track": "internal"
      }
    }
  }
}
